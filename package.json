{
  "name": "special-octo-potato",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-entities": "cross-env NODE_ENV=test jest  --verbose --runInBand --testPathPattern=src/modules/entity/useCases/.*.integration.spec.ts",
    "test-dis": "cross-env NODE_ENV=test jest  --verbose --runInBand --testPathPattern=src/modules/digitalIdentity/useCases/.*.integration.spec.ts",
    "test-roles": "cross-env NODE_ENV=test jest  --verbose --runInBand --testPathPattern=src/modules/Role/useCases/.*.integration.spec.ts",
    "test-groups": "cross-env NODE_ENV=test jest  --verbose --runInBand --testPathPattern=src/modules/group/useCases/.*.integration.spec.ts",
    "build": "tsc --project tsconfig.json",
    "start": "npm run build && npm run serve",
    "serve": "node dist/index.js",
    "ts": "nodemon src/index.ts",
    "dev": "NODE_ENV=dev nodemon --watch --inspect './**/*.ts' --exec 'ts-node' src/index.ts",
    "debug": "cross-env NODE_ENV=test node --inspect ./node_modules/jest/bin/jest -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eladex/special-octo-potato.git"
  },
  "author": "eladex",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eladex/special-octo-potato/issues"
  },
  "homepage": "https://github.com/eladex/special-octo-potato#readme",
  "devDependencies": {
    "@types/config": "0.0.39",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.1.0",
    "jest-ts-auto-mock": "^2.0.0",
    "joi": "^17.4.2",
    "mongoose": "^5.12.14",
    "morgan": "^1.10.0",
    "neverthrow": "^4.2.1",
    "p-retry": "^4.6.1",
    "supertest": "^6.1.6",
    "ts-auto-mock": "^3.3.6",
    "ts-jest": "^27.0.5",
    "ttypescript": "^1.5.12"
  }
}
